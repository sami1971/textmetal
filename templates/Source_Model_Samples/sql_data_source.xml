<?xml version="1.0" encoding="utf-8"?>

<SqlQuery xmlns="http://code.google.com/p/textmetal/rev3" key="this" order="" type="Text">	
	<Text>
		SELECT 1 as a, 2 as b;
	</Text>
	<SubQueries>
		<SqlQuery xmlns="http://code.google.com/p/textmetal/rev3" key="this" order="" type="Text">	
			<Text>
				SELECT 3 + ${a} as c, 4 - ${b} as d;
			</Text>
			<SubQueries />
		</SqlQuery>
	</SubQueries>
</SqlQuery>