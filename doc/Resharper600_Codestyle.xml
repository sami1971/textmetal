    <CustomMemberReorderingPatterns><![CDATA[<?xml version="1.0" encoding="utf-8" ?>

<Patterns xmlns="urn:shemas-jetbrains-com:member-reordering-patterns">

	<!--Do not reorder COM interfaces-->
	<Pattern>
		<Match>
			<And Weight="100">
				<Kind Is="interface"/>
				<HasAttribute CLRName="System.Runtime.InteropServices.InterfaceTypeAttribute"/>
			</And>
		</Match>
	</Pattern>

	<Pattern>
		<Match>
			<And Weight="100">
				<Or>
					<Kind Is="class"/>
					<Kind Is="struct"/>
				</Or>
				<HasAttribute CLRName="System.Runtime.InteropServices.StructLayoutAttribute"/>
			</And>
		</Match>
	</Pattern>
		
	<Pattern RemoveAllRegions="true" >

		<!--constructors and destructors -->
		<Entry>
			<Match>
				<Or>
					<Kind Is="constructor"/>
					<Kind Is="destructor"/>
				</Or>
			</Match>
			<Sort>
				<Static/>
				<Kind Order="constructor destructor" />
			</Sort>
			<Group Region="Constructors/Destructors"/>
		</Entry>


		<!-- fields and constants -->
		<Entry>
			<Match>
				<Or>
					<Kind Is="constant"/>
					<Kind Is="field"/>
				</Or>
			</Match>
			<Sort>
				<Kind Order="constant field"/>
				<Static/>
				<Readonly/>
				<Name/>
			</Sort>
			<Group Region="Fields/Constants"/>
		</Entry>

		<!-- properties, indexers, and events -->
		<Entry>
			<Match>
				<Or>
					<Kind Is="property"/>
					<Kind Is="indexer"/>
					<Kind Is="event"/>
				</Or>
			</Match>
			<Sort>
				<Kind Order="event indexer property"/>
				<Static/>
				<Readonly/>
				<Name/>
			</Sort>
			<Group Region="Properties/Indexers/Events"/>
		</Entry>

		<!-- methods and operators -->
		<Entry>
			<Match>
				<Or>
					<Kind Is="method"/>
					<Kind Is="operator"/>
				</Or>
			</Match>
			<Sort>
				<Kind Order="method operator"/>
				<Static/>
				<Readonly/>
				<Name/>
			</Sort>
			<Group Region="Methods/Operators"/>
		</Entry>

		<!--nested types-->
		<Entry>
			<Match>
				<Kind Is="type"/>
			</Match>
			<Sort>
				<Name/>
			</Sort>
			<Group Region="Classes/Structs/Interfaces/Enums/Delegates"/>
		</Entry>
		
		<!--all other members-->
		<Entry>
			<Sort>
				<Name/>
			</Sort>
			<Group Region="Members"/>
		</Entry>

	</Pattern>

</Patterns>
]]></CustomMemberReorderingPatterns>
